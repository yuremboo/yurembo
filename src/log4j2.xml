<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="WARN">
  <Appenders>
  	<Apper type="Console" name="STDOUT">
  		<Layout type="PatternLayout" pattern="%d{ISO8601} [%5p] %m at %l%n"/>
  		<Filters>
  			<Filter type
  		</Filters>
    <File name="file" fileName="logs/app.log">
      <PatternLayout pattern="%d{ISO8601} [%5p] %m at %l%n"/>
    </File>
    <File name="fileinfo" fileName="logs/info.log">
      <PatternLayout pattern="%d{ISO8601} [%5p] %m at %l%n"/>
    </File>
    <Console name="STDOUT" target="SYSTEM_OUT">
      <PatternLayout pattern="%d{ISO8601} [%5p] %m at %l%n"/>
    </Console>
  </Appenders>
  <Loggers>
    <Root level="trace">
      <AppenderRef ref="file" level="DEBUG"/>
      <AppenderRef ref="STDOUT" level="INFO"/>
      <AppenderRef ref="fileinfo" level="INFO"/>
    </Root>
  </Loggers>
</Configuration>